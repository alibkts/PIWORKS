#include<stdio.h>
int main()
{
	int a,b;
	printf("please enter the first number");
	scanf("%d",&a);
	printf("please enter the second number");
	scanf("%d",&b);
	printf("number a in first state: %d\n",a);
	printf("number b in first state: %d\n",b);
	a=a+b;
	b=a-b;
	a=a-b;
	printf("number a in second state: %d\n",a);
    printf("number b in second state: %d\n",b);
	return 0;
	
}
